{
    "compilerOptions": {
        "module": "esnext",
        "target": "es2022",
        "emitDeclarationOnly": true,
        "declaration": true,
        "composite": true,
        "incremental": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "moduleResolution": "bundler",
        "jsx": "react-jsx",
        "jsxImportSource": "@satorijs/element"
    }
}
